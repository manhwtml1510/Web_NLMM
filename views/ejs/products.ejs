
<h2>Danh sách sản phẩm</h2>

<table class="table-1">
    <tr>
        <th>ID</th>
        <th>Tên</th>
        <th>Thương hiệu</th>
        <th>Giá</th>
        <th>Mô tả</th>
        <th>Dung tích(ml)</th>
        <th>Xuất sứ</th>
        <th>Giới tính</th>
        <th>Tồn kho</th>
        <th>Hành dộng</th>
    </tr>
    <% data.forEach((product) => { %>
        <tr>
            <td><%= product.id_san_pham %></td>
            <td><%= product.ten_san_pham %></td>
            <td><%= product.thuong_hieu %></td>
            <td><%= product.gia_ban %></td>
            <td><%= product.mo_ta %></td>
            <td><%= product.dung_tich %></td>
            <td><%= product.xuat_su %></td>
            <td><%= product.phan_loai %></td>
            <td><%= product.ton_kho %></td>
            <td>
                <a href="/manage/products/edit/<%= product.id_san_pham %>" >sửa</a>
                <a>xóa</a>
            </td>
        </tr>
    <% }); %>

</table>

<button>Thêm mới</button>